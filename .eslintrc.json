{
    "plugins": ["node"],
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "rules": {
        "node/exports-style": ["error", "module.exports"],
         "node/no-missing-require": ["error", {
            "allowModules": ["express", "mongoose", "winston"],
            "tryExtensions": [".js", ".json", ".node"]
        }]
    }
}